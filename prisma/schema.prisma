generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}

model Kunde {
  id         Int       @id @default(autoincrement())
  name       String
  services   KundeService[]
}

model Service {
  id       Int       @id @default(autoincrement())
  name     String
  version  String
  kunden   KundeService[]
}

model KundeService {
  id        Int      @id @default(autoincrement())
  kunde     Kunde    @relation(fields: [kundeId], references: [id])
  kundeId   Int
  service   Service  @relation(fields: [serviceId], references: [id])
  serviceId Int
}